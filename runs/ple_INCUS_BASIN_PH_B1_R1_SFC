#PBS -S /bin/csh
#PBS -V
#PBS -q devel
#PBS -lselect=1:ncpus=1:mpiprocs=1:model=cas_ait,walltime=0:20:00
#PBS -j oe
#PBS -o /nobackupp19/swfreema/INCUS/basin_simulations/philippines_camp2ex_summer/stdout
#PBS -N INCUS_PH_B1_SFC
#PBS -m be
#PBS -r n

setenv MPI_TYPE_DEPTH 16
module load gcc/10.3 mpi-hpe/mpt.2.25 comp-intel/2020.4.304 szip/2.1.1
cd /nobackupp19/swfreema/INCUS/basin_simulations/philippines_camp2ex_summer/rams_code/RAMS/
#limit coredumpsize unlimited

# HDF5 Library
setenv LD_LIBRARY_PATH /nobackupp19/swfreema/INCUS/rams_dev/hdf5-zfp-work/HDF5-1.12.2/lib:$LD_LIBRARY_PATH
# ZFP Library
setenv LD_LIBRARY_PATH /nobackupp19/swfreema/INCUS/rams_dev/hdf5-zfp-work/zfp-1.0.0/lib:$LD_LIBRARY_PATH
# HDF5 ZFP Library
setenv LD_LIBRARY_PATH /nobackupp19/swfreema/INCUS/rams_dev/hdf5-zfp-work/H5Z-ZFP-1.1.0/lib:$LD_LIBRARY_PATH
# HDF5 Plugin path
setenv HDF5_PLUGIN_PATH /nobackupp19/swfreema/INCUS/rams_dev/hdf5-zfp-work/H5Z-ZFP-1.1.0/plugin

mpiexec ./bin.rams/rams-6.3.02 -f ./runs/RAMSIN.INCUS.BASIN.PH.R1.SFC  >& /nobackupp19/swfreema/INCUS/basin_simulations/philippines_camp2ex_summer/stdout/INCUS_BASIN_PH_R1_SFC_LOG1.txt
#./bin.dp.grib1/run_dgrib_era5.sh >& /nobackupp19/swfreema/INCUS/basin_simulations/philippines_camp2ex_summer/stdout/dgrib_log_1.txt