#limit coredumpsize unlimited

# HDF5 Library
#setenv LD_LIBRARY_PATH /nobackupp19/swfreema/INCUS/rams_dev/hdf5-zfp-work/HDF5-1.12.2/lib:$LD_LIBRARY_PATH
setenv LD_LIBRARY_PATH /nobackupp19/swfreema/INCUS/basin_simulations/philippines_camp2ex_summer/rams_code/hdf5-zfp-2/HDF5-1.10.7/lib:$LD_LIBRARY_PATH

# ZFP Library
setenv LD_LIBRARY_PATH /nobackupp19/swfreema/INCUS/rams_dev/hdf5-zfp-work/zfp-1.0.0/lib:$LD_LIBRARY_PATH
# HDF5 ZFP Library
setenv LD_LIBRARY_PATH /nobackupp19/swfreema/INCUS/rams_dev/hdf5-zfp-work/H5Z-ZFP-1.1.0/lib:$LD_LIBRARY_PATH
# HDF5 Plugin path
setenv HDF5_PLUGIN_PATH /nobackupp19/swfreema/INCUS/rams_dev/hdf5-zfp-work/H5Z-ZFP-1.1.0/plugin

